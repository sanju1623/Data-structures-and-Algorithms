package Euclidean;

public class euclidean {
	
	int gcd(int a,int b)
	{
	int output=0;
	if(b==0)
		return a;
		while(b!=0)
		{
		int temp=a/b;
		output=gcd(b,(a%b));	
		return output;
		}
		return output;
	}
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		euclidean e=new euclidean();
System.out.println(e.gcd(270,192));
 }
}
