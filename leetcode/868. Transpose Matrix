class Solution {
    public int[][] transpose(int[][] A) {
     
        int[][] output=new int[A[0].length][A.length];
        
        for(int i=0;i<A[0].length;i++){
            for(int j=0;j<A.length;j++){
                output[i][j]=A[j][i];
            }
            
        }
        
        return output;
        
    }
}
