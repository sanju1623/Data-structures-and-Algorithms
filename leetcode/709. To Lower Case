class Solution {
    public String toLowerCase(String str) {
    String output="";
    for(int i=0;i<str.length();i++){
        int a=(int)str.charAt(i);
        if(a>=65 &&a<=90){
            output=output+""+(char)(a+32);
        }else{
            output=output+str.charAt(i);
        }
        
    }
    return output;
    }
}
